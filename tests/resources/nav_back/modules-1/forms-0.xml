<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Household Member Registration</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/F1B58380-C929-42B0-9F2E-6B85E9C80D30" uiVersion="1" version="111" name="Household Member Registration">
					<name/>
					<next_visit_date/>
					<dob/>
					<member_type/>
					<member_types_with_dups/>
					<member_type_icons/>
				<subcase_0><case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><create><case_name/><owner_id/><case_type>household_member</case_type></create><update><dob/><member_type/><next_visit_date/></update><index><parent case_type="mother"/></index></case></subcase_0><case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><member_type_icons/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance>
			<instance src="jr://instance/casedb" id="casedb"/>
			<instance src="jr://instance/session" id="commcaresession"/>
			<bind nodeset="/data/name" type="xsd:string" required="true()"/>
			<bind nodeset="/data/next_visit_date" calculate="date(today() + 15)"/>
			<bind nodeset="/data/dob" type="xsd:date"/>
			<bind nodeset="/data/member_type"/>
			<bind nodeset="/data/member_types_with_dups" calculate="join(' ', /data/member_type, instance('casedb')/casedb/case[index/parent = instance('commcaresession')/session/data/case_id][@case_type = 'household_member'][@status = 'open']/member_type)"/>
			<bind nodeset="/data/member_type_icons" calculate="concat(if(contains(/data/member_types_with_dups, 'infant'), '&#128700;', ''), if(contains(/data/member_types_with_dups, 'child'), '&#128102;', ''), if(contains(/data/member_types_with_dups, 'male_partner'), '&#128697;', ''))"/>
			<itext>
				<translation lang="en" default="">
					<text id="name-label">
						<value>Member's name</value>
					</text>
					<text id="dob-label">
						<value>What is the person's date of birth?</value>
					</text>
					<text id="member_type-label">
						<value>What type of patient is this?</value>
					</text>
					<text id="member_type-infant-label">
						<value>infant</value>
					</text>
					<text id="member_type-child-label">
						<value>child</value>
					</text>
					<text id="member_type-male_partner-label">
						<value>male partner</value>
					</text>
					<text id="member_type-other-label">
						<value>other</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/member_type_icons" nodeset="/data/case/update/member_type_icons" relevant="count(/data/member_type_icons) &gt; 0"/><bind calculate="/data/meta/timeEnd" nodeset="/data/subcase_0/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/subcase_0/case/@user_id"/><setvalue event="xforms-ready" ref="/data/subcase_0/case/@case_id" value="instance('commcaresession')/session/data/case_id_new_household_member_0"/><bind calculate="/data/name" nodeset="/data/subcase_0/case/create/case_name"/><bind calculate="/data/meta/userID" nodeset="/data/subcase_0/case/create/owner_id"/><bind calculate="/data/dob" nodeset="/data/subcase_0/case/update/dob" relevant="count(/data/dob) &gt; 0"/><bind calculate="/data/member_type" nodeset="/data/subcase_0/case/update/member_type" relevant="count(/data/member_type) &gt; 0"/><bind calculate="/data/next_visit_date" nodeset="/data/subcase_0/case/update/next_visit_date" relevant="count(/data/next_visit_date) &gt; 0"/><bind calculate="/data/case/@case_id" nodeset="/data/subcase_0/case/index/parent"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<input ref="/data/name">
			<label ref="jr:itext('name-label')"/>
		</input>
		<input ref="/data/dob">
			<label ref="jr:itext('dob-label')"/>
		</input>
		<select1 ref="/data/member_type">
			<label ref="jr:itext('member_type-label')"/>
			<item>
				<label ref="jr:itext('member_type-infant-label')"/>
				<value>infant</value>
			</item>
			<item>
				<label ref="jr:itext('member_type-child-label')"/>
				<value>child</value>
			</item>
			<item>
				<label ref="jr:itext('member_type-male_partner-label')"/>
				<value>male_partner</value>
			</item>
			<item>
				<label ref="jr:itext('member_type-other-label')"/>
				<value>other</value>
			</item>
		</select1>
	</h:body>
</h:html>