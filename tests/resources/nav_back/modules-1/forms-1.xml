<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:vellum="http://commcarehq.org/xforms/vellum">
	<h:head>
		<h:title>Mother Follow-up visit</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/B715439E-73ED-4DE9-BF30-704DC82D2623" uiVersion="1" version="114" name="Mother Follow-up visit">
					<med_adherence/>
					<member_types_with_dups/>
					<member_type_icons/>
				<case xmlns="http://commcarehq.org/case/transaction/v2" case_id="" date_modified="" user_id=""><update><med_adherence/><member_type_icons/></update></case><orx:meta xmlns:cc="http://commcarehq.org/xforms"><orx:deviceID/><orx:timeStart/><orx:timeEnd/><orx:username/><orx:userID/><orx:instanceID/><cc:appVersion/></orx:meta></data>
			</instance>
			<instance src="jr://instance/casedb" id="casedb"/>
			<instance src="jr://instance/session" id="commcaresession"/>
			<bind nodeset="/data/med_adherence"/>
			<bind nodeset="/data/member_types_with_dups" calculate="join(' ', instance('casedb')/casedb/case[index/parent = instance('commcaresession')/session/data/case_id][@case_type = 'household_member'][@status = 'open']/member_type)"/>
			<bind nodeset="/data/member_type_icons" calculate="concat(if(contains(/data/member_types_with_dups, 'infant'), '&#128700;', ''), if(contains(/data/member_types_with_dups, 'child'), '&#128102;', ''), if(contains(/data/member_types_with_dups, 'male_partner'), '&#128697;', ''))"/>
			<itext>
				<translation lang="en" default="">
					<text id="med_adherence-label">
						<value>Is adhering to meds</value>
					</text>
					<text id="med_adherence-yes-label">
						<value>yes</value>
					</text>
					<text id="med_adherence-no-label">
						<value>no</value>
					</text>
				</translation>
			</itext>
		<bind calculate="/data/meta/timeEnd" nodeset="/data/case/@date_modified" type="xsd:dateTime"/><bind calculate="/data/meta/userID" nodeset="/data/case/@user_id"/><bind calculate="instance('commcaresession')/session/data/case_id" nodeset="/data/case/@case_id"/><bind calculate="/data/med_adherence" nodeset="/data/case/update/med_adherence" relevant="count(/data/med_adherence) &gt; 0"/><bind calculate="/data/member_type_icons" nodeset="/data/case/update/member_type_icons" relevant="count(/data/member_type_icons) &gt; 0"/><setvalue event="xforms-ready" ref="/data/meta/deviceID" value="instance('commcaresession')/session/context/deviceid"/><setvalue event="xforms-ready" ref="/data/meta/timeStart" value="now()"/><bind nodeset="/data/meta/timeStart" type="xsd:dateTime"/><setvalue event="xforms-revalidate" ref="/data/meta/timeEnd" value="now()"/><bind nodeset="/data/meta/timeEnd" type="xsd:dateTime"/><setvalue event="xforms-ready" ref="/data/meta/username" value="instance('commcaresession')/session/context/username"/><setvalue event="xforms-ready" ref="/data/meta/userID" value="instance('commcaresession')/session/context/userid"/><setvalue event="xforms-ready" ref="/data/meta/instanceID" value="uuid()"/><setvalue event="xforms-ready" ref="/data/meta/appVersion" value="instance('commcaresession')/session/context/appversion"/></model>
	</h:head>
	<h:body>
		<select1 ref="/data/med_adherence">
			<label ref="jr:itext('med_adherence-label')"/>
			<item>
				<label ref="jr:itext('med_adherence-yes-label')"/>
				<value>yes</value>
			</item>
			<item>
				<label ref="jr:itext('med_adherence-no-label')"/>
				<value>no</value>
			</item>
		</select1>
	</h:body>
</h:html>